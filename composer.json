{
    "name": "alcarin/alcarin",
    "description": "Alcarin MMORPG",
    "license": "BSD-3-Clause",
    "version": "0.1.0",
    "keywords": [
        "alcarin",
        "rpg"
    ],
    "homepage": "http://projectksi.com/",
    "minimum-stability" : "dev",
    "repositories" : [
        {
            "type": "package",
            "package": {
                "name": "colinmollenhour/mongodb-php-odm",
                "version": "1.1",
                "dist": {
                    "url": "https://github.com/colinmollenhour/mongodb-php-odm/zipball/master",
                    "type": "zip"
                },
                "autoload": {
                    "classmap": ["classes/"]
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "psychowico/zf2nette-debug",
                "version": "dev-master",
                "dist": {
                    "url": "https://github.com/psychowico/ZF2NetteDebug/zipball/develop",
                    "type": "zip"
                },
                "autoload": {
                    "classmap": [
                        "./"
                    ]
                }
            }
        }
    ],
    "require": {
        "php"                               : ">=5.4.0",
        "zendframework/zendframework"       : "2.2.*",
        "psychowico/assets-compiler"        : "dev-master",

        "zf-commons/zfc-twig"               : "1.0.3",
        "zf-commons/zfc-user"               : "dev-master",
        "neilime/zf2-twb-bundle"            : "dev-master",

        "zendframework/zend-developer-tools": "dev-master",
        "psychowico/zf2nette-debug"         : "dev-master",
        "colinmollenhour/mongodb-php-odm"   : "1.1",
        "evandotpro/edp-module-layouts"     : "dev-master",
        "doctrine/common"                   : ">=2.1",
        "umpirsky/twig-gettext-extractor"   : "1.1.*"
    }
}
